#version 460#extension GL_EXT_ray_tracing : requirestruct HitPayload{    int InstanceIndex;    int PrimitiveIndex;    vec2 HitAttributes;};hitAttributeEXT vec2 Attributes;layout(location = 0) rayPayloadInEXT HitPayload Hit;void main(){    Hit.InstanceIndex = gl_InstanceCustomIndexEXT;    Hit.PrimitiveIndex = gl_PrimitiveID;    Hit.HitAttributes = Attributes;}